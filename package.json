		"setupTestFrameworkScriptFile": "<rootDir>/setupTest.js",
		"moduleFileExtensions": [
			"js",
			"jsx"
		],
		"moduleDirectories": [
			"node_modules",
			"src"
		],
		"modulePaths": [
			"/src"
		],
		"moduleNameMapper": {
			"\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|glsl)$": "<rootDir>/__mocks__/fileMock.js",
			"\\.(css|less|styl)$": "<rootDir>/__mocks__/styleMock.js"
		},
		"collectCoverage": false,
		"coverageReporters": [
			"json",
			"lcov"
		]
