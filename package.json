{
	"name": "platformer",
	"version": "0.0.36",
	"description": "",
	"main": "dist/bundle.js",
	"scripts": {
		"clean": "rm -rf coverage dist",
		"test": "jest",
		"test:watch": "jest -o --watch",
		"dev": "webpack-dev-server --hot --inline",
		"build": "webpack",
		"build:sprites": "node genSpriteSheets.js",
		"functional": "karma start",
		"electron": "echo 'works in conemu not atom'&&electron dist/index.html",
		"flow:check": "flow check",
		"flow:suggest": "flow suggest src/Explosion.js",
		"flow:watch": "flow-watch",
		"coverage": "jest --coverage --watch",
		"deploy": "npm version patch && git push && git push --tags",
		"karma": "karma start --reporters spec --max_old_space_size=4096",
		"karma:single": "karma start --reporters spec --single-run",
		"karma:ci": "karma start --reporters coverage-istanbul,spec --browsers ChromeHeadless --single-run",
		"karma:headless": "karma start --reporters nyan --browsers ChromeHeadless",
		"karma:chrome": "karma start --reporters nyan --browsers Chrome",
		"karma:electron": "karma start --reporters nyan --browsers Electron --single-run",
		"electron:karma": "electron-open http://localhost:9876",
		"electron:dev": "electron http://localhost:8080/",
		"electron:debug": "electron --inspect-brk=5859 http://localhost:8080/",
		"lint": "eslint -f table src/ ",
		"lint:fix": "eslint src/ --fix",
		"lint:sort": "jscodeshift --parser flow -t node_modules/sort-class-members-codemod/index.js src/",
		"docker:alpine-node": "docker run -it -v $(pwd):/var/app mhart/alpine-node sh",
		"REMOVED precommit": "npm run test",
		"serve": "http-server dist",
		"gource": "gource -s .1 --hide filenames ."
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/digijin/platformer.git"
